# Variables de entorno para el despliegue en Render

# Backend API URL (se configurará automáticamente en Render)
REACT_APP_API_URL=https://inventario-backend.onrender.com

# Base de datos (se configurará automáticamente en Render)
DATABASE_URL=postgresql://user:password@host:port/database

# Configuración de Flask
FLASK_ENV=production
FLASK_DEBUG=false

# Puerto del servidor (se configurará automáticamente en Render)
PORT=10000
